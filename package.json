{
  "name": "@gargrave/garlib",
  "version": "0.0.1",
  "author": "Gabe Hargrave <gkhargrave@gmail.com>",
  "repository": "https://github.com/gargrave/garlib",
  "license": "MIT",
  "main": "dist/garlib.cjs.js",
  "module": "dist/garlib.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean:dist": "yarn rimraf dist",
    "prebuild": "jest && yarn clean:dist && yarn build:types",
    "build": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "release": "yarn build && yarn np --yolo",
    "lint": "eslint --ext .ts,.tsx ./src",
    "test": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.1.0",
    "np": "^5.2.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.8",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
